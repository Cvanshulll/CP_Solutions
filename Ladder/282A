#include <bits/stdc++.h>
using namespace std;
int main() {
  //std::cout << "Hello World!\n";
  int t;
  cin>>t;
  int x=0;
  string a[t];
  for(int i=0; i<t; i++)
  {
    cin>>a[i];
  }
  for(int i=0; i<t; i++)
  {
    if(a[i]=="X++" || a[i]=="+X+" || a[i]=="++X")
    {
      x++;
    }
    else
    {
      x--;
    }
  }
  cout<<x<<"\n";

} 
